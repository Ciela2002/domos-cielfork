{
  "manifest_version": 3,
  "name": "Domos",
  "version": "1.0.0",
  "description": "",
  "icons": {
    "64": "icons/domos_icon.svg",
    "48": "icons/domos_icon.svg",
    "32": "icons/domos_icon.svg"
  },

  "action": {
      "default_title": "Domos",
      "default_popup": "popup/popup.html"
  },

  "content_scripts": [
    {
      "exclude_matches": ["*://developer.mozilla.org/*"],
      "matches": ["<all_urls>"],
      "js": ["content_scripts/content.js"]
    }
  ],

  "permissions": [
      "storage"
  ]
}
