{
    "defaults": {
        "urls": ["*"],
        "timeout": 2000
    },
    "configs": [
        {
            "group": "Math",
            "command": "replace_input",
            "urls": ["*://www.desmos.com/*"],
            "timeout": 1000,
            "actions": [
                {
                    "input": "alpha",
                    "output": "\\alpha"
                },
                {
                    "input": "beta",
                    "output": "\\beta"
                },
                {
                    "event": "beforeinput",
                    "input": "epsilon",
                    "output": "\\varepsilon"
                }
            ]
        },
        {
            "group": "The Goat!",
            "command": "replace_input",
            "urls": ["*"],
            "timeout": 2000,
            "actions": [
                {
                    "input": "thegoat",
                    "output": "Domo is the goat!"
                }
            ]
        }
    ]

}
